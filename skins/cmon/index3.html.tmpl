<body>

 <div id='hes11'>

   #include $Extras.header

#set $windT_dir_dict = {"N" : "Nord", "NNE" : "Nordnordost", "NE" : "Nordost", "ENE" : "Ostnordost", "E" : "Ost", "ESE" : "Ostsüdost", "SE" : "Südost", "SSE" : "Südsüdost", "S" : "Süd", "SSW" : "Südsüdwest", "SW" : "Südwest", "WSW" : "Westsüdwest", "W" : "West", "WNW" : "Westnordwest", "NW" : "Nordwest", "NNW" : "Nordnordwest"}

#set $summary = $forecast.weather_summary('WU')

#set $dd = $forecast.label('Weather', $summary.windDir)
#set $wind_text = $windT_dir_dict.get($dd)

<div id='hescontainer'>
  <div id='popupanchor' style='float:left'></div>
     <!-- <div id='popupanchor' style='position:absolute; left:350px; top:80px;'></div> -->

<div class='hes'>
  <div class='hesbox'>
   <table class="hes_prog">
    <tr>
     <td class='hes1_wol'>
   #if $summary.clouds is not None
       #set $simg = 'xicons/' + $summary.clouds + '.png'
          <a href='forecastT.html'><img src='$simg' alt='$summary.clouds' title='$forecast.label('Weather', $summary.clouds)' /></a>
   #end if
     </td>
     <td class='hes1_wol'>
   #for $k in $summary.precip
        #set $precip_img = 'xicons/' + $k + '.png'
           <img class='precip-img' src='$precip_img' alt='$k' title='$forecast.label('Weather', $k)' /><br/>
   #end for
     </td>
     <td class='hes1_wol'>
   #for $k in $summary.obvis
        #set $obvis_img = 'xicons/' + $k + '.png'
            <img class='obvis-img' src='$obvis_img' alt='$k' title='$forecast.label('Weather', $k)' /><br/>
   #end for
     </td>
    </tr>
   </table>
   <b>Zambretti:</b> $forecast.label('Zambretti', $forecast.zambretti.code)<br />
       Prognose erstellt am:&nbsp;$forecast.label('Zambretti', $forecast.zambretti.event_ts)
   <hr />      
   <p><b>Weather Underground:</b>
    Der Himmel&nbsp;$forecast.label('Weather', $summary.clouds).&nbsp;
    Wind aus<b>&nbsp;$wind_text&nbsp;</b>mit einer Geschwindigkeit von&nbsp;$forecast.label('Weather', $summary.windSpeed).&nbsp;
    Die Tagestemperatur zwischen&nbsp;$forecast.label('Weather', $summary.tempMin)&nbsp; und&nbsp;
    $forecast.label('Weather', $summary.tempMax).&nbsp;
    Luftfeuchtigkeit von ca.&nbsp;$forecast.label('Weather', $summary.humidity).&nbsp;
   </p>
   <hr />
   <p><b>VantagePro2:</b> $latest($data_binding='wdsupp_binding').vantageForecastRule.raw</p>
   <hr />
   <p>Wetter-Cam Blick SW</p>
     <img src="xicons/wet/snapshot.jpg" alt="Wetterstation" style="width:300px; height:175px;" />
  </div>
</div> <!-- ende Prognose -->

