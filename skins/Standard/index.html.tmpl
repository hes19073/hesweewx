   <td>Wolkenuntergrenze:&nbsp;</td>
    <td class="re_data"> $current.cloudbase (ASL)</td>
  </tr>
  <tr>
    <td class="re_data">Luftdichte:</td>
   #try
       #set $luft = 287.05 * ($current.outTemp.raw + 273.15)
       #set $density_altitude = $current.barometer.raw * 100 / $luft
